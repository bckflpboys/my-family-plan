# NextAuth.js Configuration
# Base URL for NextAuth.js - make sure this has no quotes or special characters
NEXTAUTH_URL=http://localhost:5173
# Secret for NextAuth.js - used to encrypt cookies
NEXTAUTH_SECRET=your-secret-key-here

# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/my-family-plan?retryWrites=true&w=majority

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
